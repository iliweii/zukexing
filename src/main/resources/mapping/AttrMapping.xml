<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zukexing.dao.AttrDao">

    <insert id="insertAttr" parameterType="com.zukexing.pojo.Attr">
        INSERT INTO `attr` (`attr_id`, `house_id`, `bingxiang`, `xiyiji`,
                            `reshuiqi`, `kuandai`, `shafa`, `youyanji`,
                            `ranqizao`, `kezuofan`, `dianshi`, `kongtiao`,
                            `yigui`, `chaung`, `weishengjian`, `yangtai`,
                            `nuanqi`, `chuanghu`, `dianti`, `canju`, `chuju`,
                            `xidiyongju`, `wifi`, `chuifengji`)
        VALUES (NULL, #{houseId}, #{bingxiang}, #{xiyiji},
                #{reshuiqi}, #{kuandai}, #{shafa}, #{youyanji},
                #{ranqizao}, #{kezuofan}, #{dianshi}, #{kongtiao},
                #{yigui}, #{chaung}, #{weishengjian}, #{yangtai},
                #{nuanqi}, #{chuanghu}, #{dianti}, #{canju}, #{chuju},
                #{xidiyongju}, #{wifi}, #{chuifengji})
    </insert>

</mapper>