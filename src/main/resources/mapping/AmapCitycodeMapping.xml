<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zukexing.dao.AmapCitycodeDao">

    <select id="queryAdcode" parameterType="com.zukexing.pojo.AmapCitycode" resultType="string">
        SELECT adcode FROM `amap_citycode` WHERE `city_name` LIKE #{city_name}
    </select>

</mapper>